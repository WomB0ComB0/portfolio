  "sideEffects": false,
  "type": "module",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build && bun report-bundle-size.js",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "audit": "bun run prepare:audit && bunx audit-ci@^7.1.0 --critical --report-type important",
    "audit:high": "bun run prepare:audit && bunx audit-ci@^7.1.0 --high --report-type full",
    "audit:moderate": "bun run prepare:audit && bunx audit-ci@^7.1.0 --moderate --report-type summary",
    "scripts:audit": "bun run ./scripts/audit.ts",
    "scripts:check-unused": "bun run ./scripts/check-unused.ts",
    "scripts:copyright": "bun run ./scripts/copyright.ts",
    "scripts:copyright:check": "bun run ./scripts/copyright.ts --check",
    "scripts:img-to-b64": "bun run ./scripts/img-to-b64.ts",
    "scripts:lighthouse": "bun run ./scripts/light-house.ts",
    "scripts:package-cost": "bun run ./scripts/package-cost.ts",
    "scripts:remove-duplicates": "bun run ./scripts/remove-duplicate-packages.ts",
    "scripts:tree-shake": "bun run ./scripts/unused-tree-shake.ts",
    "build": "bun run scripts:package-cost && next build",
    "dev": "next dev",
    "e2e:ci": "bun run e2e:install && bunx playwright test --project=chromium",
    "e2e:install": "bunx playwright install",
    "gen": "graphql-codegen --config codegen.yml",
    "lint": "bunx biome check --write ./ || true && bunx stylelint src/styles/**/*.{css,scss,sass} --fix || true",
    "prepare:audit": "bun install --yarn > yarn.lock",
    "start": "next start",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "sanity:dev": "cd studio && sanity dev",
    "sanity:start": "cd studio && sanity start",
    "sanity:build": "cd studio && sanity build",
    "sanity:deploy": "cd studio && sanity deploy",
    "sanity:graphql:deploy": "cd studio && sanity graphql deploy"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      "defaults",
      "not dead",
      "not IE 11",
      "not IE_Mob 11",
      "maintained node versions"
    ]
